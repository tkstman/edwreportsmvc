@{
    ViewBag.Title = "EDW Reports";
}


    <h5>Choose Report Type:</h5>
    <div class="card-deck text-center">

        <a class="btn btn-outline-primary card mb-4 shadow-sm collapsed myCollapsible perfbtn" data-toggle="collapse" href="" role="button" aria-expanded="false" id="perfrep">
            <div class="card-header">
                <h4 class="my-0 font-weight-normal">Productivity Report</h4>
            </div>
        </a>
        <a class="btn btn-outline-primary card mb-4 shadow-sm collapsed myCollapsible edwrepbtn" data-toggle="collapse" href="" role="button" aria-expanded="false" id="edwrep">
            <div class="card-header">
                <h4 class="my-0 font-weight-normal">EDW Entry Report</h4>
            </div>
        </a>
    </div>
    <div class="collapse edwrepbtn" id="collapseExample">
        <h5>Choose Report Attributes:</h5>
        <div class="card-deck text-center">

            <div class="card card-body" style="display: inline;" id="droppy">
                <div class="btn-group mx-auto">
                    <button id="constituencybtn" type="button" class="btn btn-danger dropdown-toggle constituency" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Constituency
                    </button>

                    <div class="dropdown-menu constituency" role="menu" aria-labelledby="dropdownMenu">
                    </div>
                </div>

                <div class="btn-group  mx-auto">
                    <button type="button" class="btn btn-danger dropdown-toggle applicationType disabled" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Application Type
                    </button>
                    <div class="dropdown-menu applicationType">
                        
                    </div>
                </div>

               <div class="btn-group  mx-auto">
                    <button type="button" class="btn btn-danger dropdown-toggle workerType disabled" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Worker Type
                    </button>
                    <div class="dropdown-menu workerType">
                        
                    </div>
                </div><!---->

                <div class="btn-group  mx-auto">
                    <button type="button" class="btn btn-danger dropdown-toggle status disabled" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Status
                    </button>
                    <div class="dropdown-menu status">
                        
                    </div>
                </div>

                <div class="dropdown-divider"></div>
                <form id="entrydateform">
                    <div class="input-group date_input">
                        <h6>Specify Date:(Optional)</h6>
                        <div class="input-group mb-3 form-row">
                            <div class="form-group col text-left">
                                <label name="startdate" aria-label="Checkbox for following text input">Start Date:</label>
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <input id="entryspecifystartdate" type="checkbox" class="" />
                                        </div>
                                    </div>
                                    <input disabled type="date" id="entrystartdate" required pattern="\d{4}-\d{2}-\d{2}" class="form-control" aria-label="Text input with checkbox">
                                </div>
                            </div>
                            <div class="form-group col text-left">
                                <label name="enddate" aria-label="Checkbox for following text input">End Date:</label>
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">
                                            <input id="entryspecifyenddate" type="checkbox" class=""  />
                                        </div>
                                    </div>
                                    <input disabled type="date" id="entryenddate" required pattern="\d{4}-\d{2}-\d{2}" class="form-control" aria-label="Text input with checkbox">
                                 </div>       
                             </div>
                        </div>
                    </div>
                </form>

            </div>

        </div>
    </div>


<!--Performance Report Collapsible-->

<div  id="collapseExamplePerf" class="collapse perf">
    <h5>Choose Report Attributes:</h5>
    <div class="card-deck text-center">

        <div class="card card-body" style="display: inline;" id="droppy2">
            <form id="perfdateform">
                <div class="input-group date_input">
                    <h6>Specify Date:</h6>
                    <div class="input-group mb-3 form-row">
                        <div class="form-group col text-left">
                            <label name="perfstartdate" aria-label="Checkbox for following text input">Start Date:</label>
                            <div class="input-group mb-3">
                                <div class="input-group-prepend">
                                    <div class="input-group-text">
                                        <input id="perfspecifystartdate" type="checkbox" class="" />
                                    </div>
                                </div>
                                <input disabled type="date" id="perfstartdate" required pattern="\d{4}-\d{2}-\d{2}" class="form-control" aria-label="Text input with checkbox">
                            </div>
                        </div>
                        <div class="form-group col text-left">
                            <label name="perfenddate" aria-label="Checkbox for following text input">End Date:</label>
                            <div class="input-group mb-3">
                                <div class="input-group-prepend">
                                    <div class="input-group-text">
                                        <input id="perfspecifyenddate" type="checkbox" class="" />
                                    </div>
                                </div>
                                <input disabled type="date" id="perfenddate" required pattern="\d{4}-\d{2}-\d{2}" class="form-control" aria-label="Text input with checkbox">
                            </div>
                        </div>
                    </div>
                </div>
            </form>

        </div>

    </div>
</div>



    <div>
       <iframe id="ifrmReportViewer" src="" style="height:550px; width:100%;" frameborder="0" scrolling="no" ></iframe>
    </div>
